{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
        "@/*":["./src/*"],
        "@components/*":["./src/components/*"],
        "@layouts/*":["./src/layouts/*"],
        "@pages/*":["./src/pages/*"],
        "@styles/*":["./src/styles/*"],
        "@utils/*":["./src/utils/*"],
        "@assets/*":["./src/assets/*"],
        "@lib/*":["./src/lib/*"]
    }
  }
}